<!doctype html>
<html>
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>Cookbuk Caterer Service</title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" type="public/css" media="screen" href="main.css" />
        <style>
            body {
            background-color: coral;
        }
        </style>

    </head>
    <body>
        <ul>
            <% data.forEach(function(d){ %>
                <li>
                    <% if(d.url != null) { %>
                    <img src=<%= d.url%> height="180" width="300"/>
                    <% } %> <br>
                    <a href="caterer/<%= d.caterer_id %>" > <%= d.CatererName %> <br> </a>
                    Special cuisine - <% if(d.CuisineSpecializations != null) { %>
                        <%= d.CuisineSpecializations %>
                    <% } %><br>
                    Rating - <%= d.rating %>*
                </li>
                
            <% })%>
                
        </ul>
    </body>
</html>